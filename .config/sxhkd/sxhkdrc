ctrl + alt + t
	$TERMINAL
super + m
	$TERMINAL -e mutt
super + f
	$TERMINAL -e vifm
super + e
	$TERMINAL -e vim
super + b
	$BROWSER
super + n
	$TERMINAL -e newsboat
super + s
	$TERMINAL -e alsamixer
super + l
	slock
super + t
	touchpad_toggle.sh
super + g
	gimp
Print
	scrot $HOME/Pictures/Screenshots/%Y-%m-%d-%H-%M-%S.png -e 'xclip -selection clipboard -t image/png $f'
super + Print
	scrot -u $HOME/Pictures/Screenshots/windowed%Y-%m-%d-%H-%M-%S.png -e 'xclip -selection clipboard -t image/png $f'
super + w
	wpa_gui
